<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>0.0.1</Version>
    <Authors>Howl Dev</Authors>
    <Description>Contains different data structures for realtime machine learning.</Description>
		<Copyright>Copyright (c) Howl Dev 2025</Copyright>
    <PackageTags>machine learning, networks</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Cody-Howell/HowlDev.AI</RepositoryUrl>
		<PackageProjectUrl>https://wiki.codyhowell.dev/ai</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>_HowlDevLogo.png</PackageIcon>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\HowlDev.AI.Core\HowlDev.AI.Core.csproj" />
  </ItemGroup>

</Project>
